{% for _bill in bills %}
    <tr>
        <td>{{ _bill.id }}</td>
        <td>{{ _bill.number }}</td>
        <td>{{ _bill.orderIdInstance.id|default('-') }}</td>
        <td>{{ _bill.providerInstance.name|default('-') }}</td>
        <td>{{ _bill.statusInstance.name|default('-') }}</td>
        <td>{{ _bill.typeInstance.name|default('-') }}</td>
        {# <td>{{ _bill.hash }}</td> #}
        {# <td>{{ _bill.hashType }}</td> #}
        {# <td>{{ _bill.parentIdInstance.number|default('-') }}</td> #}
        <td class="text-center">
            <div class="btn-group">
                <a href="{{ path('flexphp.invoice.bills.read', {id: _bill.id}) }}" class="btn btn-sm btn-outline-light" title="{% trans from 'messages' %}action.read{% endtrans %}">
                    <i class="fa fa-eye text-dark" aria-hidden="true"></i>
                </a>

                {# <a href="{{ path('flexphp.invoice.bills.edit', {id: _bill.id}) }}" class="btn btn-sm btn-outline-light" title="{% trans from 'messages' %}action.edit{% endtrans %}"> #}
                    {# <i class="fa fa-edit text-info" aria-hidden="true"></i> #}
                {# </a> #}
            </div>
        </td>
    </tr>
{% endfor %}
